# Define the list
def new_list():
    def insert(self, item):
        node = self.head
        while node.next != null:
            node = node.next

        node.next = {}
        node.next.data = item
        node.next.prev = node

    def contains(self, item):
        node = self.head
        while node.next != null:
            if node.data == item:
                return True
            node = node.next
        return False

    def remove(self, item):
        node = self.head
        while node.next != null:
            if node.data == item:
                node.prev.next = node.next
                if node.next != null:
                    node.next.prev = node.prev
                return True
            node = node.next
        return False

    list = {}
    list.head = {}
    list.insert = move insert
    list.contains = move contains
    list.remove = move remove
    return list

list = new_list()

